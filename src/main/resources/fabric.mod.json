{
  "schemaVersion": 1,
  "id": "frycmod",
  "version": "${version}",

  "name": "Frycmod",
  "description": "Adds new tools, armors, potions, status effects, enchantments, new ore and smithing tables required for crafting",
  "authors": [
    "Wookash Fryc"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/frycmod-fabric",
    "sources": "https://github.com/Xires87/FrycMod",
    "issues": "https://github.com/Xires87/FrycMod/issues"
  },

  "license": "MIT",
  "icon": "assets/frycmod/textures/painting/fryc.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.fryc.frycmod.FrycMod"
    ],
    "client": [
      "net.fryc.frycmod.FrycModClient"
    ],
    "modmenu": [
      "net.fryc.frycmod.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "frycmod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17",
    "craftingmanipulator": ">=1.0.1",
    "cloth-config": ">=8.2.88"
  },
  "suggests": {
    "another-mod": "*"
  },
  "breaks": {
    "hammersandtables": "*",
    "endercurse": "*",
    "imbleeding": "*",
    "threepotions": "*",
    "frycmobvariants": "*"
  }
}
