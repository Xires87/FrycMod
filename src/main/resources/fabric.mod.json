{
  "schemaVersion": 1,
  "id": "frycmod",
  "version": "${version}",

  "name": "Frycmod",
  "description": "Adds compatibility between my other mods",
  "authors": [
    "Wookash Fryc"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/frycmod-fabric",
    "sources": "https://github.com/Xires87/FrycMod",
    "issues": "https://github.com/Xires87/FrycMod/issues"
  },

  "license": "MIT",
  "icon": "assets/frycmod/textures/painting/fryc.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.fryc.frycmod.FrycMod"
    ],
    "client": [
      "net.fryc.frycmod.FrycModClient"
    ],
    "modmenu": [
      "net.fryc.frycmod.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "frycmod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.21",
    "fabric-api": "*",
    "minecraft": "~1.20",
    "java": ">=17",
    "cloth-config": ">=11.0.99"
  },
  "suggests": {
    "another-mod": "*"
  }
}
