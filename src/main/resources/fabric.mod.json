{
  "schemaVersion": 1,
  "id": "frycmod",
  "version": "${version}",

  "name": "Frycmod",
  "description": "Adds new tools, armors, potions, status effects, enchantments, new ore and smithing tables required for crafting",
  "authors": [
    "Wookash Fryc"
  ],
  "contact": {
    "email": "frycmod@gmail.com"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.fryc.frycmod.FrycMod"
    ],
    "client": [
      "net.fryc.frycmod.FrycModClient"
    ]
  },
  "mixins": [
    "frycmod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17",
    "craftingmanipulator": ">=1.0.0"
  },
  "suggests": {
    "another-mod": "*"
  },
  "breaks": {
    "hammersandtables": "*",
    "endercurse": "*",
    "imbleeding": "*",
    "threepotions": "*",
    "frycmobvariants": "*"
  }
}
